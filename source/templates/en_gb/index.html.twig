            <div>
              <a href="/fitness-equipment" class="button margin--top margin--bottom--half">Explore Equipment</a>
            </div>
          </div>

          <div class="l--extraSmall--12 columns margin--bottom--none text--alignCenter homepage__hero__right">
            <img src="/assets/images/homepage/keiser_equipment_assortment_homepage_hero_background_mobile.png">
          </div>

        </div>
      </div>
    </div>
  </div>
  {# End Hero #}

