{% set entry = craft.entries.section('homepage').first() %}
{% extends "_layout" %}

{% block content %}
<article class="homepage">

  {# Start Hero #}
  <div class="homepage__hero__outerMostContainer">
    <div class="background--blpblack homepage__hero__outerContainer">
      <div class="container container--noPad">
        <div class="row homepage__hero__innerContainer">

          <div class="l--extraSmall--12 l--small--6 l--medium--5 columns margin--bottom--none homepage__hero__left">
            <h1 class="text--white text--sansSerif--black homepage__hero__left__heroCaption">Optimized fitness equipment&nbsp;for</h1>
