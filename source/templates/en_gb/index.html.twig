        <div class="homepage__customerLogoShowcase__agePerformance"></div>
        <div class="homepage__customerLogoShowcase__exos"></div>
        <div class="homepage__customerLogoShowcase__desktopOnly homepage__customerLogoShowcase__leaders"></div>
        <div class="homepage__customerLogoShowcase__desktopOnly homepage__customerLogoShowcase__fai"></div>
        <div class="homepage__customerLogoShowcase__desktopOnly homepage__customerLogoShowcase__digme"></div>
        <div class="homepage__customerLogoShowcase__desktopOnly homepage__customerLogoShowcase__ace"></div>

